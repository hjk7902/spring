<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.myapp.dao.ISampleRepository">

	<select id="selectSampleData" resultType="com.example.model.SampleVO">
	<![CDATA[
		SELECT 
			id 		AS "id", 
			name	AS "name", 
			email	AS "email"
		FROM sample
	]]>
	</select>

</mapper>